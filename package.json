    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
